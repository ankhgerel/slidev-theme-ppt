{
  "name": "slidev-theme-ppt",
  "version": "0.0.1",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.35.2",
    "codemirror-theme-vars": "^0.1.1",
    "prism-theme-vars": "^0.2.4",
    "theme-vitesse": "^0.5.0"
  },
  "devDependencies": {
    "@slidev/cli": "^0.35.2",
    "@types/node": "^18.6.3"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Open Sans",
        "mono": "Source Code Pro"
      },
      "drawings": {
        "enabled": true,
        "persist": true,
        "presenterOnly": false,
        "syncAll": true
      },
      "themeConfig": {
        "black": "#0d1117",
        "purple": "#6366f1",
        "red": "#f43f5e",
        "blue": "#1d4ed8",
        "green": "#10b981",
        "yellow": "#fbbf24"
      },
      "hightlighter": "prism",
      "download": true,
      "exportFilename": "pptData.pdf",
      "favicon": "./favicon.ico",
      "titleTemplate": "%s - __filename"
    }
  }
}
